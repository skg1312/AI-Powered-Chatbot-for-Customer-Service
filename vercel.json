{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd .. && npm run build",
  "outputDirectory": "../.next",
  "installCommand": "cd .. && npm install",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://medical-ai-chatbot-backend.onrender.com",
    "NEXT_PUBLIC_ENVIRONMENT": "production"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://medical-ai-chatbot-backend.onrender.com/api/$1"
    }
  ]
}
